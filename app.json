{
  "expo": {
    "name": "Bat",
    "slug": "frontend",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "buildNumber": "21",
      "supportsTablet": true,
      "bundleIdentifier": "com.batman.bat",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSCameraUsageDescription": "OCR 스캔을 위해 카메라 접근 권한이 필요합니다.",
        "NSPhotoLibraryUsageDescription": "이미지 선택을 위해 사진 라이브러리 접근 권한이 필요합니다.",
        "NSPhotoLibraryAdditionsUsageDescription": "이미지를 저장하기 위해 사진 라이브러리 저장 권한이 필요합니다.",
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "LSApplicationQueriesSchemes": [
          "kakaokompassauth",
          "kakaolink",
          "kakaoplus",
          "kakaonavi-sdk"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["kakaoe365864e7fe56bb2f65a88afd873b33d"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.batman.bat",
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
    },
    "plugins": [
      "expo-notifications", 
      [
        "expo-build-properties",
        {
          "ios": {
            "useHooks": true
          }
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e9972864-6164-4023-9020-a4b1f3c766a4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e9972864-6164-4023-9020-a4b1f3c766a4"
    }
  }
}